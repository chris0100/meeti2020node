<div class="contenedor proximos-eventos">

    <h1><%= nombrePagina %></h1>

    <% if (meetis.length > 0){ %>
        <div class="grid columnas-md-3 ">
            <% meetis.forEach(obj => { %>

                <%- include('partials/card-meeti.ejs', {obj: obj}) %>

            <% }) %>

        </div>
    <% } else { %>
        <p>No hay Meeti's en esta categoria</p>
    <% } %>
</div>














